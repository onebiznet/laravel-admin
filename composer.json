{
    "name": "onebiznet/laravel-admin",
    "description": "Laravel Admin Panel",
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15",
        "spatie/laravel-medialibrary": "^10.9",
        "illuminate/support": "^10.11",
        "illuminate/view": "^10.11",
        "laravel/fortify": "^1.17",
        "rappasoft/laravel-livewire-tables": "^2.14",
        "livewire/livewire": "^2.12",
        "butschster/meta-tags": "^2.3",
        "yoeunes/toastr": "^2.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OneBiznet\\Admin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "drhtoo",
            "email": "drpyaesonehtoo@gmail.com"
        }
    ],
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=admin-assets --ansi --force"
        ]
    },    
    "extra": {
        "laravel": {
            "providers": [
                "OneBiznet\\Admin\\AdminServiceProvider"
            ]
        }
    }    
}
